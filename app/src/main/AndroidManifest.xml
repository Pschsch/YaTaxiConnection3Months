<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.pschsch.pschschapps.yataxiconnection3months">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".DaggerClasses.AppInitializer"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.activities.RegistrationActivity"
            android:theme="@style/AppThemeWithActionBar"
            ></activity>
        <activity android:name=".View.activities.PrepareForRegActivity" />
        <activity android:name=".MvpViewsForExtending.MvpAppCompatActivity" />
        <activity android:name=".View.activities.PrivacyPolicyActivity" />
        <activity android:name=".View.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.MainActivity" />
    </application>

</manifest>